{
    "_from": "firefox@^77.0.1",
    "_id": "zsh-firefox@77.0.1",
    "_inBundle": false,
    "_location": "/zsh-firefox",
    "_phantomChildren": {},
    "_requested": {
        "type": "range",
        "registry": true,
        "raw": "firefox@^77.0.1",
        "name": "firefox",
        "escapedName": "firefox",
        "rawSpec": "^77.0.1",
        "saveSpec": null,
        "fetchSpec": "^77.0.1"
    },
    "_requiredBy": [],
    "_spec": "firefox@^77.0.1",
    "_where": "/root/github2/pkg-firefox",
    "author": "Mozilla Foundation",
    "bugs": {
        "url": "https://bugzilla.mozilla.org/"
    },
    "version": "1.18.2",
    "contributors": [],
    "dependencies": {},
    "deprecated": false,
    "description": "Automatic privacy is here. Download Firefox to block over 2000 trackers.",
    "devDependencies": {},
    "homepage": "https://github.com/aws/firefox",
    "keywords": [
        "web browser",
        "browser",
        "internet browser",
        "zsh"
    ],
    "license": "Mozilla Public License",
    "name": "zsh-firefox",
    "zsh-data": {
        "plugin-info": {
            "url": "https://download.mozilla.org/?product=firefox-latest-ssl&os=${${${(M)OSTYPE##linux}:+linux64}:-${${(M)OSTYPE##darwin}:+osx}}&lang=en-US",
            "message": "A snippet providing Firefox Developer Edition."
        },
        "zplugin-ices": {
            "default": {
                "is-snippet": "",
                "lucid": "",
                "as": "command",
                "pick": "**/firefox(|-bin)(|.exe)",
                "nocompile": "",
		"mv": "%ID% ff.pkg",
                "atclone": "local -A map; map=( '(#i)linux*' tar.bz2 '(#i)(cygwin|win|wsl)*' exe '(#i)darwin*' dmg ); local ext=\"${map[(k)$OSTYPE]}\"; zpextract ff.pkg $ext --norm ${${OSTYPE:#(#i)(darwin|cygwin|win|wsl)*}:+--move};",
                "atpull": "%atclone"
            },
            "bgn": {
                "required": "bgn",
                "is-snippet": "",
                "lucid": "",
                "as": "null",
                "nocompile": "",
		"mv": "%ID% ff.pkg",
                "atclone": "local -A map; map=( '(#i)linux*' tar.bz2 '(#i)(cygwin|win|wsl)*' exe '(#i)darwin*' dmg ); local ext=\"${map[(k)$OSTYPE]}\"; zpextract ff.pkg $ext --norm ${${OSTYPE:#(#i)(darwin|cygwin|win|wsl)*}:+--move};",
                "atpull": "%atclone",
                "sbin": "**/firefox(.exe|-bin) -> firefox"
            }
        }
    }
}
